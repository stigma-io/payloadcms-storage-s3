{
 "compilerOptions": {
  "baseUrl": ".",
  "declaration": true,
  "declarationMap": true,
  "target": "ES2022",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
  "allowJs": true,
  "checkJs": false,
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "jsx": "preserve",
  "lib": [
   "DOM",
   "DOM.Iterable",
   "ES2022"
  ],
  "noEmit": true,
  "outDir": "./dist",
  "resolveJsonModule": true,
  "rootDir": ".",
  "skipLibCheck": true,
  "sourceMap": true,
  "strict": false,
  "types": [
   "jest",
   "node",
   "@types/jest"
  ],
  "incremental": true,
  "isolatedModules": true,
  "plugins": [
   {
    "name": "next"
   }
  ]
 },
 "exclude": [
  "dist",
  "build",
  "temp",
  "node_modules"
 ],
 "composite": true,
 "include": [
  "app",
  ".next/types/**/*.ts",
  "scripts/**/*.ts",
  "instrumentation.ts",
  "sentry.server.config.ts",
  "sentry.client.config.ts"
 ]
}